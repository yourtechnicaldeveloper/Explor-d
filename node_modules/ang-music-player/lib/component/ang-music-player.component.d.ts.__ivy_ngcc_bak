import { OnInit, EventEmitter } from '@angular/core';
import { AudioPlyerOptions } from '../audioPlayer';
export declare class AngMusicPlayerComponent extends AudioPlyerOptions implements OnInit {
    width: any;
    height: any;
    backgroundColor: any;
    audioTimeColor: any;
    audioTitleColor: any;
    volumeSliderColor: any;
    timeSliderColor: any;
    audioList: any[];
    next: boolean;
    previous: boolean;
    shuffle: boolean;
    repeat: boolean;
    scrollTitle: boolean;
    playButtonColor: string;
    pauseButtonColor: string;
    nextButtonColor: string;
    previousButtonColor: string;
    repeatButtonColor: string;
    activeRepeatButtonColor: string;
    volumeButtonColor: string;
    muteButtonColor: string;
    nextEvent: EventEmitter<any>;
    previousEvent: EventEmitter<any>;
    repeatEvent: EventEmitter<any>;
    shuffleEvent: EventEmitter<any>;
    seekEvent: EventEmitter<any>;
    selectedAudio: any;
    currentAudioIndex: number;
    repeatActive: boolean;
    isError: boolean;
    isShuffle: boolean;
    volumeBeforeMute: any;
    constructor();
    ngOnInit(): void;
    nextAudio(): void;
    previousAudio(): void;
    seekAudio(seekAudioValue: any): void;
    repeatAudio(): void;
    volumeChange(volume: any): void;
    muteAudio(): void;
    initiateAudioPlayer(): void;
}
