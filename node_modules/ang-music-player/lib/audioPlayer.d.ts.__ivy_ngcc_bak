import { ElementRef, EventEmitter } from '@angular/core';
import { Subject } from 'rxjs';
export declare class AudioPlyerOptions {
    totalAudioLength: any;
    currentAudioTime: number;
    isAudioLoaded: boolean;
    isAudioPlaying: boolean;
    isRepeat: boolean;
    audioVolume: number;
    isAudioEnded: Subject<unknown>;
    isMute: boolean;
    playEvent: EventEmitter<any>;
    pauseEvent: EventEmitter<any>;
    audioPlayer: ElementRef;
    options(): void;
    play(): void;
    pause(): void;
    getAudioLength(): void;
}
